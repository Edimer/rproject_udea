exp1 <- c()
for (i in 1:500) {
muestra = replicate(sample(moneda, size = 1), n = i)
exp1[i] = prop.table(table(muestra))[1]
}
x11()
plot(exp1, type = "l", xlab = "Lanzamientos", ylab = "Probabilidad")
